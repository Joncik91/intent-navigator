{
  "name": "intent-navigator",
  "displayName": "Intent Navigator",
  "description": "A sovereignty-first tool to help builders reconnect with their intent and reduce false starts",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:intentNavigator.latestIntentLine",
    "onCommand:intentNavigator.peekIntent",
    "onCommand:intentNavigator.archiveWithReason",
    "workspaceContains:README.md"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "intentNavigator.latestIntentLine",
        "title": "Latest Intent Line",
        "category": "Intent Navigator"
      },
      {
        "command": "intentNavigator.peekIntent",
        "title": "Peek: What user pain...?",
        "category": "Intent Navigator"
      },
      {
        "command": "intentNavigator.archiveWithReason",
        "title": "Archive with Reason",
        "category": "Intent Navigator"
      },
      {
        "command": "intentNavigator.showControlIndex",
        "title": "Show Control Index",
        "category": "Intent Navigator"
      }
    ],
    "keybindings": [
      {
        "command": "intentNavigator.latestIntentLine",
        "key": "ctrl+shift+l",
        "mac": "cmd+shift+l",
        "when": "editorTextFocus && resourceLangId == markdown"
      },
      {
        "command": "intentNavigator.peekIntent",
        "key": "ctrl+shift+p",
        "mac": "cmd+shift+p",
        "when": "editorTextFocus && resourceLangId == markdown"
      },
      {
        "command": "intentNavigator.archiveWithReason",
        "key": "ctrl+shift+a",
        "mac": "cmd+shift+a",
        "when": "editorTextFocus && resourceLangId == markdown"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.41.0",
    "typescript": "^4.9.5"
  }
}